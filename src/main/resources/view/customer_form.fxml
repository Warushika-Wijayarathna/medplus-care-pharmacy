<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane onKeyPressed="#onFKeyPressed" prefHeight="841.0" prefWidth="1544.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.medpluscarepharmacy.controller.CustomerFormController">
   <children>
      <TableView fx:id="customerTable" layoutX="55.0" layoutY="393.0" onMouseClicked="#onMouseClickAction" prefHeight="413.0" prefWidth="1463.0">
         <columns>
            <TableColumn fx:id="colCustId" prefWidth="106.0" text="Customer Id" />
            <TableColumn fx:id="colCustName" prefWidth="297.0" text="Customer Name" />
            <TableColumn fx:id="colContact" prefWidth="238.0" text="Contact No" />
            <TableColumn fx:id="colEmail" prefWidth="597.0" text="Email" />
            <TableColumn fx:id="colAction" prefWidth="223.0" text="Action" />
         </columns>
      </TableView>
      <Label layoutX="55.0" layoutY="124.0" prefHeight="40.0" prefWidth="243.0" text="Customer Name">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <JFXTextField fx:id="custTxt" layoutX="298.0" layoutY="124.0" onKeyReleased="#onNameReleased" prefHeight="40.0" prefWidth="259.0" />
      <Label layoutX="55.0" layoutY="174.0" prefHeight="40.0" prefWidth="243.0" text="Contact No">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <JFXTextField fx:id="contactTxt" layoutX="298.0" layoutY="174.0" onKeyReleased="#onContactReleased" prefHeight="40.0" prefWidth="259.0" />
      <Label layoutX="55.0" layoutY="227.0" prefHeight="40.0" prefWidth="243.0" text="Email">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <JFXTextField fx:id="emailTxt" layoutX="298.0" layoutY="227.0" onKeyReleased="#onEmailReleased" prefHeight="40.0" prefWidth="259.0" />
      <JFXButton fx:id="addBtn" layoutX="588.0" layoutY="235.0" onAction="#addBtnOnAction" prefHeight="40.0" prefWidth="124.0" style="-fx-background-color: GREEN;" text="add" textFill="WHITE" />
      <JFXTextField fx:id="searchBar" layoutX="55.0" layoutY="43.0" onKeyPressed="#onSearchCustomer" prefHeight="40.0" prefWidth="504.0" promptText="Search Customer" />
      <ImageView fitHeight="279.0" fitWidth="296.0" layoutX="934.0" layoutY="78.0" onMouseClicked="#onClickAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icon/Untitled design (45).png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
