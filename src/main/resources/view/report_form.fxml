<AnchorPane onKeyPressed="#onFKeyPressed" prefHeight="841.0" prefWidth="1544.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.medpluscarepharmacy.controller.ReportFormController">
   <children>
      <Label layoutX="40.0" layoutY="123.0" prefHeight="39.0" prefWidth="155.0" text="Test">
